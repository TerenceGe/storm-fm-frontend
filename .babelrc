{
  "presets": [
    ["latest", {
      "es2015": {
        "loose": true,
        "modules": false
      }
    }],
    "react",
    "stage-0",
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    "transform-react-constant-elements",
    "transform-react-inline-elements",
    "transform-decorators-legacy",
    ["react-intl", {
        "messagesDir": "./static/messages/"
    }]
  ],
  "env": {
    "test": {
      "presets": [
        "es2015"
      ],
      "pulgins": [
        ["module-resolver", {
          "root": ["./shared"]
        }]
      ]
    }
  }
}
