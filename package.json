  "scripts": {
    "build": "NODE_ENV=production webpack --progress --colors --watch --config tools/webpack.config.prod.js",
    "build:dev": "NODE_ENV=development webpack --progress --colors --config tools/webpack.config.prod.js --watch",
    "client": "http-server static/ -p 8000",
    "server": "NODE_ENV=production nodemon static/app.js",
    "start": "webpack-dev-server -d --config tools/webpack.config.dev.js --history-api-fallback --hot --inline --progress --color --port 3000 --host 0.0.0.0",
    "test": "mocha --require babel-register --require babel-polyfill --require ignore-styles --require test/helpers/setup.js --recursive",
    "cover": "nyc --reporter=lcov --reporter=text --reporter=html --require babel-register --extension .js npm test",
    "lint": "eslint client server shared test"
  },
