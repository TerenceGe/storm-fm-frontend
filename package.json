  "scripts": {
    "build": "rimraf static && npm run build:server && npm run build:client",
    "build:dev": "rimraf static && cross-env NODE_ENV=development TARGET=web webpack --progress --colors",
    "build:client": "cross-env NODE_ENV=production TARGET=web webpack --progress --colors",
    "build:server": "cross-env NODE_ENV=production TARGET=node webpack --progress --colors",
    "server": "cross-env NODE_ENV=production nodemon static/app.js",
    "start": "cross-env NODE_ENV=development TARGET=web webpack-dev-server -d --history-api-fallback --hot --inline --progress --port 3008 --host 0.0.0.0",
    "test": "mocha --require babel-register --require babel-polyfill --require test/helpers/setup.js --recursive",
    "cover": "nyc --reporter=lcov --reporter=text --reporter=html --require babel-register --extension .js npm test",
    "lint": "eslint client server shared test"
  },
